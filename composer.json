{
    "name": "waygou/helpers",
    "description": "General helpers, configuration setters for any Laravel web application.",
    "type": "library",
    "keywords": [
        "laravel",
        "php",
        "waygou"
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\Helpers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\Helpers\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waygou",
            "email": "bruno.falcao@waygou.com"
        }
    ],
    "minimum-stability": "dev"
}
